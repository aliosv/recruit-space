{
    "paths" : {
        "./desktop.bundles/static/": "static/",
        "./desktop.bundles/static/css/": "./",
        "./desktop.bundles/static/js/": "./"
    },
    "freeze_paths" : {
        "**/*.jpg": "desktop.bundles/static/i",
