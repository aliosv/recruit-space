        "**/*.ttf": "desktop.bundles/static/fonts",
        "**/*.woff": "desktop.bundles/static/fonts",
        "**/*.css": "desktop.bundles/static/css",
        "**/*.js": "desktop.bundles/static/js",
        "**/": "desktop.bundles/static"
    }
}
